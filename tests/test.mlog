declareLink(foreshadow1);


declareInputVar(test123, 67890);

fn getPos(obj : Var) -> (Var, Var) {
  return (sensor(obj, @x), sensor(obj, @y));
}

fn main() {
  let player = radar(@this, Target.player, Target.any, Target.any, 1, Sort.distance);
  if (player === null) {
    end;
  }
  let (playerX, playerY) = getPos(player);
  for (let linkNum = 0; linkNum < @links; linkNum = linkNum + 1) {
    controlShoot(getLink(linkNum), playerX, playerY, true);
  }
}